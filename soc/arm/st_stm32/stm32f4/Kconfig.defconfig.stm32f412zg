if SOC_STM32F412ZG

config SOC
	default "stm32f412zx"

config NUM_IRQS
	default 97

if GPIO_STM32

config GPIO_STM32_PORTF
	default y

config GPIO_STM32_PORTG
	default y

endif # GPIO_STM32

config ENTROPY_STM32_RNG
	default y
	depends on ENTROPY_GENERATOR

endif # SOC_STM32F412ZG

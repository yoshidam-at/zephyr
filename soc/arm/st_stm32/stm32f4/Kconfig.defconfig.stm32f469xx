if SOC_STM32F469XX

config SOC
	default "stm32f469xx"

config NUM_IRQS
	default 93

if GPIO_STM32

config GPIO_STM32_PORTF
	default y

config GPIO_STM32_PORTG
	default y

config GPIO_STM32_PORTI
	default y

config GPIO_STM32_PORTJ
	default y

config GPIO_STM32_PORTK
	default y

endif # GPIO_STM32

config ENTROPY_STM32_RNG
	default y
	depends on ENTROPY_GENERATOR

endif # SOC_STM32F469XX

if SOC_STM32F417XX

config SOC
	default "stm32f417xx"

config NUM_IRQS
	default 82

if GPIO_STM32

config GPIO_STM32_PORTF
	default y

config GPIO_STM32_PORTG
	default y

config GPIO_STM32_PORTI
	default y

endif # GPIO_STM32

config ENTROPY_STM32_RNG
	default y
	depends on ENTROPY_GENERATOR

config CRYPTO_STM32
	default y
	depends on CRYPTO

endif # SOC_STM32F417XX

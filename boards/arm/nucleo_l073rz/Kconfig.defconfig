if BOARD_NUCLEO_L073RZ

config BOARD
	default "nucleo_l073rz"

config UART_2
	default y
	depends on UART_CONSOLE

config I2C_1
	default y
	depends on I2C

if SPI

config SPI_1
	default y

config SPI_STM32_INTERRUPT
	default y

endif # SPI

config ADC_1
	default y
	depends on ADC

endif # BOARD_NUCLEO_L073RZ
